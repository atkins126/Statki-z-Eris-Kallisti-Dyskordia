    <skala_proporcjonalna>1,1</skala_proporcjonalna>
      <!-- 0, ujemne działają jak dodatnie. -->

    <współrzędne>
      <x>0,0</x>
      <y>-0,3</y>
      <z>-0,5</z>
    </współrzędne>

    <wychylenie__lewo_prawo>1,1</wychylenie__lewo_prawo>
    <obrót__lewo_prawo>1,1</obrót__lewo_prawo>
      <!-- Dodatnie w lewo (lewa burta). -->
    <wychylenie__przód_tył>1,1</wychylenie__przód_tył>
      <!-- -(minus) w górę, +(plus) w dół. -->
      <!-- 0, ujemne działają, większe wartości obniżane są poprzez wielokrotność 180. -->
  </ster>

  <torpedy_wyrzutnia lufy_ilość="1">
    <kolor>
      <r>0</r>
      <g>1</g>
      <b>0,5</b>
      <a>0</a>
    </kolor>

    <skala_proporcjonalna>1</skala_proporcjonalna>

    <współrzędne>
      <x>-0,1</x>
      <y>1,6</y>
      <z>-0,2</z>
    </współrzędne>

    <obrót__lewo_prawo>123,456</obrót__lewo_prawo>
      <!-- Dodatnie w lewo (lewa burta). -->

    <obrót_kąt_zablokowany_do>1,1</obrót_kąt_zablokowany_do>
    <obrót_kąt_zablokowany_od>2,2</obrót_kąt_zablokowany_od>

